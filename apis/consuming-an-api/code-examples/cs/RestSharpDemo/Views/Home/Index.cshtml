@model RestSharpDemo.Models.IndexViewModel

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Look at all them repos</h1>
</div>

<div>
    <ul>
        @foreach (var repo in Model.Repos)
        {
            <li>@repo.full_name</li>
            <li>@repo.description</li>
            <br/>
        }
    </ul>
</div>